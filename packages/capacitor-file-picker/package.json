{
  "name": "@mymonero/capacitor-file-picker",
  "version": "2.1.3",
  "description": "This plugin presents the native UI for picking a file.",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "author": "Jose Martinez (with tweaks from Karl Buys)",
  "license": "MIT",
  "dependencies": {
    "@capacitor/core": "^3.0.2"
  },
  "devDependencies": {
    "@capacitor/android": "^3.0.2",
    "@capacitor/ios": "^3.0.2",
    "rimraf": "^3.0.0",
    "typescript": "^3.2.4"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "MymoneroCapacitorFilePicker.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mymonero-utils"
  },
  "bugs": {
    "url": "https://github.com/mymonero-utils"
  },
  "publishConfig": {
    "access": "public"
  }
}
